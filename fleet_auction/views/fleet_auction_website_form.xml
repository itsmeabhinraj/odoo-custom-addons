<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="web_form_template">
       <t t-call="website.layout">
           <div class="container">
               <h3>Enter your bid</h3>
               <t t-foreach="auction_details" t-as="auction">
                   <form action="/fleet_auctions/bid_form_submit" enctype="multipart/form-data">
                       <label>Vehicle</label>
                       <t t-esc="auction.vehicle_name_id.name"></t><br/>
<!--                       <label>Auction ID</label>-->  
<!--                       <t t-esc="auction.auction_id"></t><br/>-->
    <!--<label>Name</label>
                       <t t-esc="auction.vehicle_name_id.name"></t><br/>
                    <input id="auction_id" type="text" name="auction_id"/>-->
                       <label>Start Price</label>
                       <t t-esc="auction.start_price"></t>
<!--                       <input id="bid_amount" type="float" name="auction.start_price"/><br/>-->
                       <label>Date</label>
                       <input id="bid_date" type="date" name="bid_date"/><br/>
<!--                       <label>Currency</label>-->
<!--                       <input id="currency_id" type="text" name="currency_id"/><br/>-->
                       <label>Bid Price</label>
                       <input id="bid_price" type="text" name="bid_price"/><br/>
                       <label>Phone</label>
                       <input id="phone" type="integer" name="phone"/><br/>

    <!--                   <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px">-->
    <!--                        <span class="s_website_form_label_content">Name</span>-->
    <!--                        <span class="s_website_form_mark"> *</span>-->
    <!--                   </label>-->
    <!--                   <div class="col-sm">-->
    <!--                       <input id="name" type="text" class="form-control s_website_form_input" name="auction_id" required="1"/>-->
    <!--                   </div>-->
                       <button type="submit" class="btn btn-primary">Submit</button>
                   </form>
               </t>
           </div>
       </t>
    </template>
</odoo>




